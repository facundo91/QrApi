{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=qrdb;Trusted_Connection=True;"
  },
  "MongoOptions": {
    "DefaultConnection": "mongodb://localhost:27017",
    "Database": "qrdb"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SwaggerOptions": {
    "JsonRoute": "swagger/{documentName}/swagger.json",
    "Description": "QR API",
    "UIEndpoint": "v1/swagger.json"
  },
  "AllowedHosts": "*",
  "IoCOptions": {
    "DalImplementation": "json"
  },
  "FeatureManagement": {
    "EndpointFlag": true,
    "MethodFlag": false,
    "AbTestFlag": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    }
  }
}